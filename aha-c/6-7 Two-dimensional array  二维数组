# include <stdio.h>

int main()
{
    int a[3][4];
    int b=0;
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<4;j++)
        {
            a[i][j]=b;
            b++;
        }
    }
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<4;j++)
        {
            printf("%d ",a[i][j]);
        }printf("\n");
    }
    return 0;
}
